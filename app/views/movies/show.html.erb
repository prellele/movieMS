<% content_for :header do %>
  <%= javascript_include_tag :favorite %>
<% end %>
<%= render :partial => "single", :locals => { :movie => @movie} %>
<% if (!@movie.trailer_url.nil?) %>
  <iframe style="margin-left:123px;" width="640" height="360" src="https://www.youtube.com/embed/<%= @movie.trailer_url %>" frameborder="0" allowfullscreen></iframe>
<% end %>
